.properties',this.properties)\r\n            UserService.update(this.userInfos.nickname, this.userInfos.mail, this.properties, this.mail_1).then(res=>{\r\n                console.log(res)\r\n                console.log('프로필 수정완료!')\r\n                alert('수정완료')\r\n            }).catch(e=>{\r\n                console.log(\"user update 오류::::\", e)\r\n            })\r\n        },\r\n\r\n        \r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped> \r\n    .profile_title {\r\n      font-weight: bold;\r\n      margin-bottom: 2%;\r\n    }\r\n    .profile_ontent {\r\n       margin-bottom: 1%;  \r\n    }\r\n    .profile_underLine {\r\n        border-color: black;\r\n        margin-bottom:4%;\r\n        width: 90%;\r\n    }\r\n    \r\n</style>"],"sourceRoot":""}]}